/* 轮播图 */
/* swiper宽度100%，高度跟图片等比例,比如图片750px*340px,swiper就设置为750rpx,340rpx */
swiper{
  width:750rpx;
  height: 340rpx;
}
.swiper swiper-item image{
  width: 100%;
}
/* 楼层页面 */
/* 清除浮动 */
.index_floor{
  overflow: hidden;
}
.floor_group{
  width: 100%;
}
.floor_title{
  width: 100%;
}
.floor_title image{
  width: 100%;
  margin: 20rpx 0;
}
/* 每个图片宽度皆为33.33% */
.floor_list navigator{
  width: 33.33%;
  float: left;
}
/* 倒数几个 */
.floor_list navigator:nth-last-child(-n+4){
  height: 27.7271vw;
  border-left: 10rpx solid #fff;
}
.floor_list navigator:nth-child(2){
  border-bottom: 10rpx solid #fff;
}
.floor_list navigator:nth-child(3){
   border-bottom: 10rpx solid #fff;
}
.floor_list navigator image{
  width: 100%;
  height: 100%;
}